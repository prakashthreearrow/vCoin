<div class="container-fluid">
    <div class="row page-titles">
        <div class="col-md-5 align-self-center">
            <h4 class="text-themecolor">
                Subscribtions
            </h4>
        </div>
        <div class="col-md-7 align-self-center text-right">
            <div class="d-flex justify-content-end align-items-center">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="<%= siteURL %>dashboard">Home</a></li>
                    <li class="breadcrumb-item active">Subscribtions</li>
                </ol>
            </div>
        </div>
    </div>
    <%- include('../common/flashMessage.ejs') %>
        <div class="row">

            <div class="col-12">

                <div class="card">

                    <div class="card-body">
                        <div class="text-right"><input type="text" name="datetimes" class="daterange  form-control w-50 "/></div>
                        <div class="table-responsive">
                            <table id="subscribtions" class="table table-striped table-bordered contact-list"
                                cellspacing="0" width="100%">
                                <thead>
                                    <tr>

                                        <th>Business</th>
                                        <th>Store</th>
                                        <th>Plan</th>
                                        <th>Subscription Id</th>
                                        <th>Subscription End Date</th>
                                        <th>Created At</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.subscriptionData.forEach(function (ele) { %>
                                        <tr>
                                            <td>
                                                <% if(ele.Store.Business.name){ %>
                                                    <p class="text-center">
                                                        <%= ele.Store.Business.name %>
                                                    </p>
                                                    <% } else { %>
                                                        <p class="text-center">-</p>
                                                        <% } %>

                                            </td>
                                            <td>
                                                <% if(ele.Store.name){ %>
                                                    <p class="text-center">
                                                        <%= ele.Store.name %>
                                                    </p>
                                                    <% } else { %>
                                                        <p class="text-center">-</p>
                                                        <% } %>
                                            </td>
                                            <td>
                                                <% if(ele.Plan){ %>
                                                    <p class="text-center">
                                                        <%= ele.Plan.title %>
                                                    </p>
                                                    <% } else { %>
                                                        <p class="text-center">-</p>
                                                        <% } %>
                                            </td>
                                            <td>
                                                <% if(ele.subscribtion_id){ %>
                                                    <p class="text-center">
                                                        <%= ele.subscribtion_id %>
                                                    </p>
                                                    <% } else { %>
                                                        <p class="text-center">-</p>
                                                        <% } %>
                                            </td>


                                            <td>
                                                <% if(ele.subscription_end_date){ %>
                                                    <p class="text-center">
                                                        <%= Helper.dateFormatWithTime(ele.subscription_end_date) %>
                                                    </p>
                                                    <% } else { %>
                                                        <p class="text-center">-</p>
                                                        <% } %>
                                            </td>
                                            <td>
                                                <% if(ele.createdAt){ %>
                                                    <p class="text-center">
                                                        <%= Helper.dateFormatWithTime(ele.createdAt) %>
                                                    </p>
                                                    <% } else { %>
                                                        <p class="text-center">-</p>
                                                        <% } %>
                                            </td>



                                        </tr>
                                        <%}); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</div>

<!-- Modal  -->
<!-- Modal  -->



<script src="../../public/node_modules/bootstrap-daterangepicker/daterangepicker.js"></script>

<script type="text/javascript">
      $('.daterange').daterangepicker();
    $('#subscribtions').DataTable();
   
</script>