<!DOCTYPE html>
<html lang="en">
<style>
    .error {
        color: red;
    }

    .spinner-border-sm {
        margin-bottom: 4px !important;
    }
</style>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image/png" sizes="16x16" href="../../public/images/favicon.png">
    <link rel="stylesheet" type="text/css"
        href="../../public/node_modules/datatables.net-bs4/css/dataTables.bootstrap4.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css"
        href="../../public/node_modules/datatables.net-bs4/css/responsive.dataTables.min.css">
    <link href="../../public/dist/css/style.min.css" rel="stylesheet">
    <link href="../../public/dist/css/pages/layout.css" rel="stylesheet">
    <script src="../../public/node_modules/moment/moment.js"></script>
    <link href="../../public/node_modules/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
        integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
        integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>

<body class="skin-default fixed-layout skin-blue-dark">
    <div class="preloader">
        <div class="loader">
            <div class="loader__figure"></div>
            <p class="loader__label"></p>
        </div>
    </div>
    <div id="main-wrapper">
        <header class="topbar">
            <nav class="navbar top-navbar navbar-expand-md navbar-dark">
                <div class="navbar-header">
                    <a class="navbar-brand d-flex justify-content-center" href="<%= siteURL %>dashboard">
                        Vibecoin
                    </a>
                </div>
                <div class="navbar-collapse">
                    <ul class="navbar-nav topnav-right d-flex justify-content-between">
                        <li class="nav-item"><a
                                class="nav-link sidebartoggler d-none d-lg-block d-md-block waves-effect waves-dark"
                                href="javascript:void(0)"><i class="icon-menu"></i></a></li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="left-sidebar">
            <div class="scroll-sidebar">
                <div class="user-profile">
                    <div class="user-pro-body">
                        <div>
                            <% if(locals.user.profilePhoto) { %>
                                <img src="<%= locals.user.profilePhoto %>" alt="user-img" class="img-circle">
                                <% } else { %>
                                    <img src="../../public/images/users/avatar.jpg" alt="user-img" class="img-circle">
                                    <% } %>
                        </div>
                        <div class="dropdown">
                            <a href="javascript:void(0)" class="dropdown-toggle u-dropdown link hide-menu"
                                data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                <% if(locals.user) { %>
                                    <span class="caret">
                                        <%= locals.user.full_name %>
                                    </span>
                                    <% } %>

                            </a>
                            <div class="dropdown-menu animated flipInY">
                                <a href="<%= siteURL %>edit_profile" class="dropdown-item"><i class="ti-user"></i>
                                    <span> My
                                        Profile</span></a>
                                <a href="<%= siteURL %>change_password" class="dropdown-item"><i class="ti-wallet"></i>
                                    <span>Change Password</span>
                                </a>
                                <a href="<%= siteURL %>logout" class="dropdown-item"><i class="ti-power-off"></i>
                                    <span>Logout</span> </a>
                            </div>
                        </div>
                    </div>
                </div>
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <li> <a href="<%= siteURL %>user" class="waves-effect"><i class="ti-user"></i><span
                                    class="hide-menu">My
                                    Users</span></a>
                        </li>
                        </li>
                        <li> <a class="has-arrow waves-effect waves-dark" href="javascript:void(0)"
                                aria-expanded="false"><i class="ti-settings"></i><span
                                    class="hide-menu">Setting</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li> <a href="<%= siteURL %>plans" class="waves-effect"><i class="ti-menu"></i>
                                        Plans</a>
                                </li>
                                <li><a href="<%= siteURL %>emailTemplate" class="waves-effect"><i class="ti-menu"></i>
                                        Email Template</a></li>
                                <li> <a href="<%= siteURL %>get_vibecoin_limit" class="waves-effect"><i
                                            class="ti-menu"></i>
                                        Vibecoin Limit</a>
                                </li>
                            </ul>
                        </li>
                        <li> <a class="has-arrow waves-effect waves-dark" href="javascript:void(0)"
                                aria-expanded="false"><i class="fas fa-chart-bar"></i><span
                                    class="hide-menu">Reports</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li><a href="<%= siteURL %>vibecoin_purchase_reports"><i class="ti-menu"> </i>Vibecoin
                                        Purchase</a></li>
                                <li><a href="<%= siteURL %>vibecoin_transaction_reports"><i class="ti-menu">
                                        </i>Vibecoin Transaction</a></li>
                                <li><a href="<%= siteURL %>vibecoin_referrals_reports"><i class="ti-menu"></i> Vibecoin
                                        Referrals</a></li>
                                <li> <a href="<%= siteURL %>subscribtions" class="waves-effect"><i class="ti-menu"></i>
                                        Subscribtions</a>
                                </li>
                            </ul>
                        </li>
                        <li><a href="<%= siteURL %>mint-password" class="waves-effect"><i
                                    class="fas fa-address-card"></i><span data-toggle="modal" data-target="#myModal"
                                    class="hide-menu">Mint
                                    Vibecoin</span></a>
                        </li>
                        <li><a href="<%= siteURL %>minter-role-password" class="waves-effect"><i
                                    class="fas fa-address-card"></i><span data-toggle="modal" data-target="#myModal"
                                    class="hide-menu">Minter Role
                                    Vibecoin</span></a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>

        <div class="page-wrapper">
            <%- body %>
        </div>
        <footer class="footer text-left">
            Â© 2021 TADA Vibecoin
        </footer>
    </div>
    <script src="../../public/node_modules/jquery/jquery-3.2.1.min.js"></script>
    <script src="../../public/node_modules/popper/popper.min.js"></script>
    <script src="../../public/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="../../public/dist/js/perfect-scrollbar.jquery.min.js"></script>
    <script src="../../public/dist/js/waves.js"></script>
    <script src="../../public/dist/js/sidebarmenu.js"></script>
    <script src="../../public/node_modules/sticky-kit-master/dist/sticky-kit.min.js"></script>
    <script src="../..//public/node_modules/sparkline/jquery.sparkline.min.js"></script>
    <script src="../../public/dist/js/custom.min.js"></script>
    <script src="../../public/node_modules/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="../..//public/node_modules/datatables.net-bs4/js/dataTables.responsive.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.1/js/dataTables.buttons.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.1/js/buttons.flash.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.32/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.32/vfs_fonts.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.1/js/buttons.html5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.1/js/buttons.print.min.js"></script>
    <script src="../../public/node_modules/tinymce/tinymce.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js"
        integrity="sha512-37T7leoNS06R80c8Ulq7cdCDU5MNQBwlYoy1TX/WUsLFC2eYNqtKlV0QjH7r8JpG/S0GUMZwebnVFLPd6SU5yg=="
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script type="text/javascript">

        $(function () {
            $("#live").perfectScrollbar();
            $("#task1").perfectScrollbar();
            $("#task2").perfectScrollbar();
            $("#task3").perfectScrollbar();
            $('.buttons-copy, .buttons-csv, .buttons-print, .buttons-pdf, .buttons-excel').addClass('btn btn-primary mr-1');
        });
    </script>
    <%- script %>
</body>

</html>