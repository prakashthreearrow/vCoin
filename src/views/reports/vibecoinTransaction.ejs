<div class="container-fluid">
    <div class="row page-titles">
        <div class="col-md-5 align-self-center">
            <h4 class="text-themecolor">
                Vibecoin Transaction
            </h4>
        </div>
        <div class="col-md-7 align-self-center text-right">
            <div class="d-flex justify-content-end align-items-center">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="<%= siteURL %>dashboard">Home</a></li>
                    <li class="breadcrumb-item active">Vibecoin Transaction</li>
                </ol>
            </div>
        </div>
    </div>
    <%- include('../common/flashMessage.ejs') %>
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table id="vibecoinTransaction" class="table table-striped table-bordered contact-list"
                                cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>From</th>
                                        <th>From Type</th>
                                        <th>To</th>
                                        <th>To Type</th>
                                        <th>Amount</th>
                                        <th>Transaction Date</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.vibecoinTransactionInfo.forEach(function (vibecoinTransaction) { %>
                                        <tr>
                                            <td>
                                                <p class="text-center">
                                                    <% if(vibecoinTransaction.from_id){ %>
                                                        <%= vibecoinTransaction.from_id %>
                                                            <% } else { %>-<% } %>
                                                </p>
                                            </td>
                                            <td>
                                                <p class="text-center">
                                                    <% if (vibecoinTransaction.from_type===1) { %>
                                                        <span class="">User</span>
                                                        <% } else if (vibecoinTransaction.from_type===2) { %>
                                                            <span class="">Business</span>
                                                            <% }else { %>
                                                                <span class="">Store</span>
                                                                <% } %>
                                                </p>
                                            </td>
                                            <td>
                                                <p class="text-center">
                                                    <% if(vibecoinTransaction.to_id){ %>
                                                        <%= vibecoinTransaction.to_id %>
                                                            <% } else { %>-<% } %>
                                                </p>
                                            </td>
                                            <td>
                                                <p class="text-center">
                                                    <% if (vibecoinTransaction.to_type===1) { %>
                                                        <span class="">User</span>
                                                        <% } else if (vibecoinTransaction.to_type===2) { %>
                                                            <span class="">Business</span>
                                                            <% }else { %>
                                                                <span class="">Store</span>
                                                                <% } %>
                                                </p>
                                            </td>
                                            <td>
                                                <p class="text-center">
                                                    <% if(vibecoinTransaction.amount){ %>
                                                        <%= Helper.amountFormat(vibecoinTransaction.amount) %>
                                                            <% } else { %>-<% } %>
                                                </p>
                                            </td>
                                            <td>
                                                <p class="text-center">
                                                    <% if(vibecoinTransaction.createdAt){ %>
                                                        <%= Helper.dateFormatWithTime(vibecoinTransaction.createdAt) %>
                                                            <% } else { %>-<% } %>
                                                </p>
                                            </td>
                                        </tr>
                                        <%}); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</div>

<script>
    $('#vibecoinTransaction').DataTable();
</script>